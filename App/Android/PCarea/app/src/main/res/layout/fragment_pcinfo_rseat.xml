<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutLine1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="10dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toRightOf="@id/btnSeat1"
            app:layout_constraintTop_toTopOf="@id/btnSeat1"
            android:layout_marginLeft="5dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toRightOf="@id/btnSeat2"
            app:layout_constraintTop_toTopOf="@id/btnSeat1"
            android:layout_marginLeft="70dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toRightOf="@id/btnSeat3"
            app:layout_constraintTop_toTopOf="@id/btnSeat1"
            android:layout_marginLeft="5dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginRight="10dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/btnSeat1"
            android:background="@drawable/shape2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutLine2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/layoutLine1">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat6"
            android:layout_width="50dp"
            android:layout_height="50dp"
