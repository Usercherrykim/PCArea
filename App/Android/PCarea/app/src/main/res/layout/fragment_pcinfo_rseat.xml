            app:layout_constraintTop_toTopOf="@id/btnSeat12"
            android:layout_marginLeft="70dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat14"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toRightOf="@id/btnSeat13"
            app:layout_constraintTop_toTopOf="@id/btnSeat12"
            android:layout_marginLeft="5dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat15"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginRight="10dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/btnSeat12"
            android:background="@drawable/shape2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutLine4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/layoutLine3">
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat16"
            android:layout_width="50dp"
            android:layout_height="50dp"
