            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat8"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toRightOf="@id/btnSeat7"
            app:layout_constraintTop_toTopOf="@id/btnSeat6"
            android:layout_marginLeft="70dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat9"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toRightOf="@id/btnSeat8"
            app:layout_constraintTop_toTopOf="@id/btnSeat6"
            android:layout_marginLeft="5dp"
            android:background="@drawable/shape2" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSeat10"
            android:layout_width="50dp"
            android:layout_height="50dp"
